{
  "name": "Blazebyte Shoutbox",
  "version": "5.0.0.2",
  "manifest_version": 2,
  "description": "Chat on Blazebyte.org quickly and easily with the blazebyte shoutbox extension.",
  "browser_action": {
    "default_icon": "img/logo.png",
    "default_popup": "popup.html"
  },
  "icons": {
    "128": "img/logo128.png",
    "16": "img/logo16.png",
    "32": "img/logo32.png",
    "48": "img/logo48.png"
   },
  "permissions": [
    "http://blazebyte.org/"
  ],
  "background": {
  	"persistant":true,
  	"page": "background.html"
  },
  "options_page": "options.html",
  "homepage_url": "http://blazebyte.org/community/"
}