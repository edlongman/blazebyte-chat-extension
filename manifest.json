{
  "name": "Blazebyte Shoutbox",
  "version": "3.5.4.4",
  "description": "Chat on Blazebyte.org quickly and easily with the blazebyte shoutbox extension.",
  "browser_action": {
    "default_icon": "img/logo.png",
    "popup": "popup.html"
  },
  "icons": {
    "128": "img/logo128.png",
    "16": "img/logo16.png",
    "32": "img/logo32.png",
    "48": "img/logo48.png"
   },
  "permissions": [
    "tabs",
    "http://blazebyte.org/"
  ],
  "background_page":"background.html"
}